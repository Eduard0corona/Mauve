{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Mauve Log",
	"patterns": [
		{
			"include": "#keywords"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.mvl",
				"match": "(?i)(exception|error|warning|success)"
			}]
		}
	},
	"scopeName": "source.mvl"
}